#include<iostream>
using namespace std;

int main()
{
  int num, i, j, k;

  cout<<"\n Enter number of rows: ";
  cin>>num;

  for(i=1; i<=num; ++i)
  {
     for(j=0; j<=num-i; ++j)//loop for space 
     {
         cout<<"_ ";
     }

     for()
     {
         
     }
     cout<<endl;
  }

  return 0;
}